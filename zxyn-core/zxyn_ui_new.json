{
  "namespace": "z_main",

  "button_panel_1": { "type": "panel", "size": [ "fill", "100%" ]},
  "button_panel_2": { "type": "panel", "size": [ "100%", 25 ]},

  "xbox_panel@neko_c.bg_3": {
    "layer": 2,
    "alpha": 1,
    "size": [  "100%", 20 ],
    "color": [0.09, 0.13, 0.17 ],
    "$warna_text": [ 1.0, 1.0, 1.0 ], //HITAM
    "$ukuran_font": 0.7,
    "$gaya_font": "PoppinsMedium",
    "anchor_from": "center",
    "anchor_to": "center",
    "$panel_img_sign_size": [ 15 , "100%" ],
    "$panel_img_not_sign_size": [ 15 , "100%" ],

    "$panel_text_sign_size": [ "fill" , "100%" ],
    "$panel_text__not_sign_size": [ "fill" , "100%" ],

    "controls": [
      {
        "xbox_sign_in": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%" , "100%" ],
          "controls": [
            {
              "spasi": {
                "type": "panel",
                "size": [ 3, 3 ]
              }
            },
            {
              "panel_img_sign": {
                "type": "panel",
                "size": "$panel_img_sign_size",
                "bindings": [
                  {
                    "binding_name": "#sign_in_visible",
                    "binding_name_override": "#visible" 
                  }
                ],
                "controls": [
                  {
                    "image": {
                      "type": "image",
                      "layer": 11,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "size": [ 15, 15 ],
                      "texture": "textures/neko/main_icon/xbox_b"
                    }
                  }
                ]
              }
            },
            {
              "panel_img_not_sign": {
                "type": "panel",
                "size": "$panel_img_not_sign_size",
                "bindings": [
                  {
                    "binding_name": "#feedback_visible",
                    "binding_name_override": "#visible" 
                  }
                ],
                "controls": [
                  {
                    "image": {
                      "type": "image",
                      "layer": 11,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "size": [ 15, 15 ],
                      "texture": "textures/neko/main_icon/xbox_a"
                    }
                  }
                ]
              }
            },
            {
              "spasi": {
                "type": "panel",
                "size": [ 5, 5 ]
              }
            },
            {
              "panel_text_sign": {
                "type": "panel",
                "size": "$panel_text_sign_size",
                "controls": [
                  {
                    "label": {
                      "type": "label",
                      "layer": 11,
                      "size": [ "100% - 20px", 20 ],
                      "font_scale_factor": "$ukuran_font",
                      "font_type": "$gaya_font",
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "color": "$warna_text",
                      "text": "xbox.sign_text_true",
                      "bindings": [
                        {
                          "binding_name": "#feedback_visible",
                          "binding_name_override": "#visible" 
                        }
                      ]
                    }
                  },
                  {
                    "label": {
                      "type": "label",
                      "layer": 11,
                      "size": [ "100%", 20],
                      "font_scale_factor": "$ukuran_font",
                      "font_type": "$gaya_font",
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "color": "$warna_text",
                      "text": "xbox.sign_text_false",
                      "bindings": [
                        {
                          "binding_name": "#sign_in_visible",
                          "binding_name_override": "#visible" 
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "top_panel": {
    "type": "panel",
    "size": [ "100%", 20 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      {
        "logo_and_button": {
          "type": "stack_panel",
          "size": [ "100%", "100%" ],
          "orientation": "horizontal",
          "controls": [
            {
              "logo_panel": {
                "type": "panel",
                "size": [ "50%", "100%" ],
                "controls": [
                  {
                    "zxyn_ui_logo": {
                      "type": "image",
                      "size": [ 80, 27 ],
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "texture": "textures/neko/logo2"
                    }
                  }
                ]
              }
            },
            {
              "button_panel": {
                "type": "panel", 
                "size": [ "fill", "100%" ],
                "controls": [
                  {
                    "button_stack": {
                      "type": "stack_panel",
                      "size": [ "100%", "100%" ],
                      "orientation": "horizontal",
                      "controls": [
                        { "a@z_main.button_panel_1": { "controls": [ { "store_button_2@zxyn.store_button_2": {}}]}},
                        { "b@z_main.button_panel_1": { "controls": [ { "achievements_button_2@zxyn.achievements_button_2": {}}]}},
                        { "c@z_main.button_panel_1": { "controls": [ { "profile_button@zxyn.profile_button": {}}]}},
                        { "d@z_main.button_panel_1": { "controls": [ { "settings_button_2@zxyn.settings_button_2": {}}]}}
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "left_panel": {
    "type": "panel",
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "size": [ 90, "100% - 40px" ],
    "controls": [
      {
        "neko_stack_panel": {
          "type": "stack_panel",
          "orientation": "vertical",
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "size": [ "100%", "100%"],
          "controls": [
            {
              "neko_panel_b": {
                "type": "panel",
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "size":[ "100%", "10%" ],
                "controls": [ 
                  {
                    "neko_stack_panel": {
                      "type": "stack_panel",
                      "orientation": "vertical",
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "size": [ "100%", "100%"],
                      "controls": [
             /*           {
                          "panel_event@zxyn_main_ui.panel_event": {}
                        } */
                      ]
                    }
                  }
                ]
              }
            },
            {
              "neko_panel_a": {
                "type": "panel",
                "size":[ "100%", "fill" ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "controls": [ 
                  {
                    "neko_stack_panel": {
                      "type": "stack_panel",
                      "orientation": "vertical",
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "controls": [
                      ]
                    }
                  }
                ]
              }
            },
            {
              "neko_panel_b": {
                "type": "panel",
                "size":[ "100%", "100%c" ],
                "controls": [ 
                  {
                    "neko_stack_panel": {
                      "type": "stack_panel",
                      "orientation": "vertical",
                      "anchor_from": "bottom_left",
                      "anchor_to": "bottom_left",
                      "size": [ "100%", "100%c"],
                      "controls": [
                        { "spacer": { "type": "panel", "size": [ 5,5 ]}},
                        { "xbox_panel@z_main.xbox_panel": {}},
                        { "spacer": { "type": "panel", "size": [ 3,3 ]}},
                        { "neko_panel_2@z_main.button_panel_2": { "controls": [ { "start_button@zxyn.start_button": {}}]}}
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "bottom_middle": {
    "type": "panel",
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "alpha": 1,
    "size": [ "100% - 110px" , 20 ],
    "controls": [
      {
        "more_button": {
          "type": "panel",
          "size": [ "100%", "100%" ],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "controls": [
            {
              "top_stack": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "anchor_from": "top_right",
                "anchor_to": "top_right",
                "controls": [
                  { "minecraft_version@loli.minecraft_version": {}},
                  {
                    "invite_button@zxyn.invite_button": { "bindings": [{ "binding_name": "#feedback_visible", "binding_name_override": "#visible" }]}
                  },
                  { 
                    "spacer": { "type": "panel", "size": [ 3, 3 ]}
                  },
                  {
                    "feedback_button@zxyn.feedback_button": { "bindings": [{ "binding_name": "#feedback_visible", "binding_name_override": "#visible" }]}
                  },
                  { 
                    "spacer": { "type": "panel", "size": [ 3, 3 ]}
                  },
                  {
                    "quit_button@zxyn.quit_button": { "ignored": "$win10_edition" }
                  },
                  { 
                    "spacer": { "type": "panel", "size": [ 3, 3 ]}
                  },
                  {
                    "login_xbox@zxyn.login_xbox": { "bindings": [{ "binding_name": "#sign_in_visible", "binding_name_override": "#visible" }]}
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "zxyn_core_main_ui": {
    "type": "panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ "100% - 10px", "100% - 10px"],
    "layer": 1,
    "controls": [ 
      {
        "bottom_middle@z_main.bottom_middle": {}
      },
      {
        "top_panel@z_main.top_panel": {}
      },
      {
        "left_panel@z_main.left_panel": {}
      }
    ]
  }
}